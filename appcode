#!/bin/bash
#Generated by JetBrains Toolbox 1.24.11947 at 2022-05-13T08:41:10.658177

declare -a ideargs=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    ideargs+=("\"$o\"")
  else
    ideargs+=("$o")
  fi
done

open -na "/Users/andreluft/Library/Application Support/JetBrains/Toolbox/apps/AppCode/ch-0/221.5591.52/AppCode.app/Contents/MacOS/appcode" $wait --args "${ideargs[@]}"